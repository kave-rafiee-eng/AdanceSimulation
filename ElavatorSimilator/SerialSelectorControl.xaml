<UserControl x:Class="ElavatorSimilator.SerialSelectorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ElavatorSimilator"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <StackPanel Orientation="Horizontal" Margin="5">
        <ComboBox x:Name="SerialComboBox" Width="120" Margin="5"/>
        <Button x:Name="OpenButton" Content="Open" Width="80" Margin="5"/>
        <Button x:Name="colseButton" Content="CLose" Width="80" Margin="5"/>
    </StackPanel>
    
</UserControl>
